{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "private": true,
  "description": "A simple Discord bot template featured with an awesome new thing on Discord.",
  "repository": "https://github.com/zhycorp/discord-bot-template",
  "license": "BSD-3-Clause",
  "author": "Zhycorp <support@zhycorp.net>",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "start:dev": "ts-node -r dotenv/config src/bot.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@zhycorp/eslint-config/typescript"
  },
  "dependencies": {
    "@discordjs/voice": "^0.8.0",
    "date-fns": "^2.26.0",
    "discord.js": "^13.3.1",
    "got": "^11.8.3",
    "tslib": "^2.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@zhycorp/eslint-config": "^6.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.7.0",
    "bufferutil": "^4.0.5",
    "erlpack": "github:discord/erlpack",
    "libsodium-wrappers": "^0.7.9",
    "utf-8-validate": "^5.0.7",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
